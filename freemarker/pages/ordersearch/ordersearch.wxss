}
.search-bar{
  padding:20rpx 30rpx ;
}
.searc-result{
  padding: 0 25rpx;
  background: #fff;
}
.result-item{
   padding: 25rpx;
   font-size: 28rpx;
   border-bottom: 1rpx solid #e6e6e6;
   display: flex;
   justify-content: space-between;
}
.result-item:last-child{
  border: 0;
}
.no-route{

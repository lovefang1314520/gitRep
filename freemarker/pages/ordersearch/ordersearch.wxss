}
.no-route image{
  margin-top: 100rpx;
  margin-bottom: 20rpx;
  width: 260rpx;
  height: 259rpx;
}
.no-txt-b{
  font-size: 35rpx;
  margin-bottom: 20rpx;
}
.no-txt-s{
  font-size: 30rpx;

.search-bar {
  padding: 0rpx 30rpx;
  height:60rpx;
  font-size: 28rpx;
  background: #f5f5f5;
  display: flex;
  align-items: center;
  position: relative;
}
.search-bar .saoyisao{
  width: 26rpx;
  height:26rpx;
  position: absolute;
  left: 40rpx;
  padding: 10rpx;
}
.search-bar  text{
   padding-left: 20rpx;
}
.search-bar input{
  padding-left:60rpx; 
  height: 60rpx;
  line-height: 60rpx;
  background: #fff;
  border-radius:30rpx; 
  box-sizing: border-box;
  border: 1rpx solid #d9d9d9;
  flex:1;
}